<ui version="4.0">
<class>CentralWidget</class>
<widget class="QWidget" name="centralWidget">
<layout class="QVBoxLayout" name="verticalLayout">
    <item>
        <widget class="TickerInput" name="tickerInput"/>
    </item>
    <item>
        <widget class="QSplitter" name="splitter">
            <property name="orientation">
                <enum>Qt::Horizontal</enum>
            </property>
            <widget class="ChartPane" name="chartPane"/>
            <widget class="RightPane" name="rightPane"/>
        </widget>
    </item>
</layout>
</widget>
<includes>
    <include location="local">frontend/tickerinput/tickerinput.h</include>
    <include location="local">frontend/chartpane/chartpane.h</include>
    <include location="local">frontend/rightpane/rightpane.h</include>
</includes>
<resources/>
<connections/>
</ui>
